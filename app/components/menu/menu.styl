@require 'pics'

.menu
  .title
    @extend .plus
    padding 0 7px 0 16px
    background-position left 1px
    background-repeat no-repeat
    background-size 14px
    font-weight bold
    cursor pointer
    color: red
  .list
    display none
    margin 0
    a
      color blue
  &.open
    .list
      display block
    .title
      @extend .minus

